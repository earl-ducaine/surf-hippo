alias rm rm
find . \( -name '.*~' \) -exec rm {} \;
find . \( -name '.*#' \) -exec rm {} \;
find . \( -name '*~' \) -exec rm {} \;
find . \( -name '*#' \) -exec rm {} \;
find . \( -name '*.sparcf' \) -exec rm {} \;
find . \( -name '*.fasl' \) -exec rm {} \;
find . \( -name '*.x86f' \) -exec rm {} \;
alias rm rm -i

